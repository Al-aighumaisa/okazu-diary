            for (let cookie of cookies) {
                var cookiesArray = cookie.split('=');
                if (cookiesArray[0].trim() == 'language') {
                    language= cookiesArray[1];
                }
            }
            var text="";
            if(typeof language === "undefined") {
               language = "0";
            }
            if(language === "0") {
                text = `${userName ? userName + 'さん ' : ''}マイページを見る`;
            }else if(language === "1") {
               text = `${userName ? userName : ''} 查看我的頁面`;
