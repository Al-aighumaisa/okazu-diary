{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "checkJs": true,
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "lib": [],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": [],
    "noErrorTruncation": true,
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "sourceMap": true,
    "target": "ES2020"
  },
  "typeAcquisition": {
    "enable": false
  }
}
